<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scoder.vin.web.api.mapper.extension.ExConfigMapper">
    <select id="getByKey" resultType="com.scoder.vin.web.api.domain.extension.ExConfig">
      select *
      from v_config
      where `key` = #{key}
      limit 1
    </select>
</mapper>